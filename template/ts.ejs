/**
* <%=title %> 
* <%=description %>
*/
import { request } from './request';
export default class <%= className %> {
  <% methods.forEach(method => {%>
    /** 
    * <%=method.summary %>
    * @method <%=method.method %>
    * @name <%=className %><%=method.summary%>
    */
    <%=method.methodName %>(data, option){
      return request({
        url:'<%=method.path %>',
        method: '<%=method.method %>',
        data: data
      }, option)
    }
  <%})%>
}